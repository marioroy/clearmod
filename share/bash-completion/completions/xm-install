# bash completion script - XanMod on Clear project

_xm-install()
{
    local cur prev words cword
    _init_completion || return

    COMPREPLY=( $(compgen -W '
        edge-custom edge-default edge-preempt
        lts-custom lts-default lts-preempt
        rt-custom rt-preempt rt-preempt_rt' -- "$cur")
    )
} &&
    complete -F _xm-install xm-install

