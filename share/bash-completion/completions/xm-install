# bash completion script - XanMod on Clear project

__xm-install()
{
    local cur prev words cword
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"

    COMPREPLY=(
        $(compgen -W 'clear    bore
                      clear-rt bore-rt' -- "$cur")
    )
} &&
    complete -F __xm-install xm-install

